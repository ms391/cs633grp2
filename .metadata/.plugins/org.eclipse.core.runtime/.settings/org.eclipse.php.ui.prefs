org.eclipse.php.ui.editor.templates=<?xml version\="1.0" encoding\="UTF-8" standalone\="no"?><templates><template autoinsert\="true" context\="php" deleted\="false" description\="create storage client for blob api" enabled\="true" name\="createBlobStorageClient">\t$$host \= Microsoft_WindowsAzure_Storage\:\:URL_CLOUD_BLOB;   &\#13;\r\n    $$usePathStyleUri \= true; &\#13;\r\n&\#13;\r\n    $$retryPolicy \= Microsoft_WindowsAzure_RetryPolicy\:\:retryN(10, 250);&\#13;\r\n    &\#13;\r\n    $$blobStorageClient \= new Microsoft_WindowsAzure_Storage_Blob(&\#13;\r\n                                    $$host,&\#13;\r\n                                    ${accountName},&\#13;\r\n                                    ${accountKey},&\#13;\r\n                                    $$usePathStyleUri,&\#13;\r\n                                    $$retryPolicy&\#13;\r\n                                    );</template><template autoinsert\="true" context\="php" deleted\="false" description\="create storage client for queue api" enabled\="true" name\="createQueueStorageClient">$$host \= Microsoft_WindowsAzure_Storage\:\:URL_CLOUD_QUEUE;&\#13;\r\n$$usePathStyleUri \= true;&\#13;\r\n&\#13;\r\n$$retryPolicy \= Microsoft_WindowsAzure_RetryPolicy\:\:retryN(10, 250);&\#13;\r\n&\#13;\r\n$$queueStorageClient \= new Microsoft_WindowsAzure_Storage_Queue(&\#13;\r\n\t\t\t\t\t\t  $$host,&\#13;\r\n\t\t\t\t\t\t  ${accountName},&\#13;\r\n\t\t\t\t\t\t  ${accountKey},&\#13;\r\n\t\t\t\t\t\t  $$usePathStyleUri,&\#13;\r\n\t\t\t\t\t\t  $$retryPolicy&\#13;\r\n\t\t\t\t\t\t  );\t\t\t\t\t\t\t\t</template><template autoinsert\="true" context\="php" deleted\="false" description\="create storage client for table api" enabled\="true" name\="createTableStorageClient">$$host \= Microsoft_WindowsAzure_Storage\:\:URL_CLOUD_TABLE;&\#13;\r\n$$usePathStyleUri \= true;&\#13;\r\n&\#13;\r\n$$retryPolicy \= Microsoft_WindowsAzure_RetryPolicy\:\:retryN(10, 250);&\#13;\r\n&\#13;\r\n$$tableStorageClient \= new Microsoft_WindowsAzure_Storage_Table(&\#13;\r\n\t\t\t\t\t\t  $$host,&\#13;\r\n\t\t\t\t\t\t  ${accountName},&\#13;\r\n\t\t\t\t\t\t  ${accountKey},&\#13;\r\n\t\t\t\t\t\t  $$usePathStyleUri,&\#13;\r\n\t\t\t\t\t\t  $$retryPolicy&\#13;\r\n\t\t\t\t\t\t  );\t\t\t\t\t\t\t\t</template><template autoinsert\="true" context\="php" deleted\="false" description\="get multiple messages from queue" enabled\="true" name\="getMessages">${dollar}${messages} \= ${dollar}${queueStorageClient}-&gt;getMessages(${dollar}${queueName}, ${dollar}${count}); // retrieve multiple messages at once&\#13;\r\nforeach (${dollar}${messages} as ${dollar}${message})&\#13;\r\n{&\#13;\r\n    echo ${dollar}${message}-&gt;MessageText . "\\r\\n";&\#13;\r\n}</template><template autoinsert\="true" context\="php" deleted\="false" description\="list blobs in a specific container" enabled\="true" name\="listBlobs">${dollar}${blobs} \= ${dollar}${blobStorageClient}-&gt;listBlobs(${containerName});     &\#13;\r\nforeach (${dollar}${blobs} as ${dollar}${blob})&\#13;\r\n{&\#13;\r\n\techo "${dollar}${blob}-&gt;Name " . "&lt;br/&gt;";        &\#13;\r\n}</template><template autoinsert\="true" context\="php" deleted\="false" description\="list containers in blob storage" enabled\="true" name\="listContainers">${dollar}${containers} \= ${dollar}${blobStorageClient}-&gt;listContainers();    &\#13;\r\nforeach (${dollar}${containers} as ${dollar}${container})&\#13;\r\n{&\#13;\r\n\techo "${dollar}${container}-&gt;Name &lt;br/&gt;";&\#13;\r\n}</template><template autoinsert\="true" context\="php" deleted\="false" description\="list tables in table storage" enabled\="true" name\="listTables">${dollar}${result} \= ${dollar}${tableStorageClient}-&gt;listTables();&\#13;\r\nforeach (${dollar}${result} as ${dollar}${table})&\#13;\r\n{&\#13;\r\n  echo "- ${dollar}${table}-&gt;Name &lt;br/&gt;";&\#13;\r\n}&\#13;\r\n&\#13;\r\n</template></templates>
